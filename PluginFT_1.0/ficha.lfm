                        return y    
                    elseif x == T then
                        y = string.format(math.floor((INT + FM + RF + RM + DES + FF)/2)..' turnos')
                        return y
                    elseif x == M then
                        y = string.format(math.floor((INT + FM)/2)..' turnos')
                        return y
                     end
