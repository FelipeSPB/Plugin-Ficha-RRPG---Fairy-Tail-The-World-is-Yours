                        return y
                    elseif x == 'Transformação' then
                        y = string.format('+'..transform..' em qualquer teste de acordo com o mestre')
                        return y            
                    elseif x == 'Vampirismo' then
                        y = string.format('Adicionará +'..vampirismo..' no teste de Dano no alvo.')
                        return y
                    end    
                end
