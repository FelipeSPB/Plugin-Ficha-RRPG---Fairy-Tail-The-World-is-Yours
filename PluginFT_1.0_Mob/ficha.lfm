<!--Função Bonus Racial-->>
<dataLink fields="{'bonusRF','bonusDES','bonusRM','bonusINT','bonusFF','bonusFM','mob'}">
             <event name="onChange">
               local bonusRACA = sheet.mob
               if bonusRACA == 'Humano' then
               		sheet.bonusRF = 0
               		sheet.bonusDES = 0
               		sheet.bonusRM = 1
               		sheet.bonusINT = 1
               		sheet.bonusFF = 0
               		sheet.bonusFM = 0
               elseif bonusRACA == 'Demônio' then
               		sheet.bonusRF = 0
               		sheet.bonusDES = 0
               		sheet.bonusRM = 1
               		sheet.bonusINT = 0
               		sheet.bonusFF = 0
               		sheet.bonusFM = 1
               	elseif bonusRACA == 'Besta' then	
              		sheet.bonusRF = 1
               		sheet.bonusDES = 1
               		sheet.bonusRM = 0
               		sheet.bonusINT = -1
               		sheet.bonusFF = 2
               		sheet.bonusFM = 0
               	elseif bonusRACA == 'Drow' then
               		sheet.bonusRF = -1
               		sheet.bonusDES = 0
               		sheet.bonusRM = 0
               		sheet.bonusINT = 2
               		sheet.bonusFF = -1
               		sheet.bonusFM = 2
                elseif bonusRACA == 'Espectro' then
                    sheet.bonusRF = 0
                    sheet.bonusDES = 0
                    sheet.bonusRM = 2
                    sheet.bonusINT = 0
                    sheet.bonusFF = -2
                    sheet.bonusFM =  2
                elseif bonusRACA == 'Fada' then
                    sheet.bonusRF = -1 
                    sheet.bonusDES = 2
                    sheet.bonusRM = 2
                    sheet.bonusINT = 0
                    sheet.bonusFF = -3
                    sheet.bonusFM =  2
                elseif bonusRACA == 'Gigante' then
                    sheet.bonusRF = 2 
                    sheet.bonusDES = -2
                    sheet.bonusRM = 0
                    sheet.bonusINT =0
                    sheet.bonusFF = 4
                    sheet.bonusFM = -2
                elseif bonusRACA == 'Humanoides' then
                    sheet.bonusRF = 3 
                    sheet.bonusDES = 0
                    sheet.bonusRM = -1
                    sheet.bonusINT = -1
                    sheet.bonusFF = 3
                    sheet.bonusFM = -2
                elseif bonusRACA == 'Insetos' then
                    sheet.bonusRF = -2 
                    sheet.bonusDES = 3
                    sheet.bonusRM = -2
                    sheet.bonusINT = 1
                    sheet.bonusFF = 1
                    sheet.bonusFM = 2
                elseif bonusRACA == 'Robôs' then
                    sheet.bonusRF = -1 
                    sheet.bonusDES = 0
                    sheet.bonusRM = -1
                    sheet.bonusINT = 2
                    sheet.bonusFF = -2
                    sheet.bonusFM =  4		
               else
               		sheet.bonusRF = 0
               		sheet.bonusDES = 0
               		sheet.bonusRM = 0
               		sheet.bonusINT = 0
               		sheet.bonusFF = 0
               		sheet.bonusFM = 0
               end	
               
	        </event>
</dataLink>

