<!--Função CAF --> 
<dataLink fields="{'resfisica', 'bonusRF', 'destreza','bonusDES'}">
             <event name="onChange">
               sheet.caf = (((tonumber(sheet.resfisica) or 0) + 
                              (tonumber(sheet.bonusRF) or 0) + 
                              (tonumber(sheet.destreza) or 0)+
                            (tonumber(sheet.bonusDES) or 0))/2)+
                            10;
               x = sheet.caf
               sheet.caf = math.floor(x)     
                
	        </event>
</dataLink>
